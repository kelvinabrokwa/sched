{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "d3": true,
    "React": true,
    "ReactDOM": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "strict": [0],
    "quotes": [2, "single"],
    "curly": [0],
    "no-use-before-define": [0],
    "comma-dangle": [0],
    "new-cap": [0],
    "one-var": [0],
    "one-var-declaration-per-line": [0],
    "react/jsx-first-prop-new-line": [0],
    "jsx-quotes": [0],
    "newline-per-chained-call": [0],
    "no-param-reassign": [0],
    "react/jsx-boolean-value": [0],
    "react/jsx-no-bind": [0]
  }
}
